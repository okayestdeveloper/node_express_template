{
  "name": "node_express_template",
  "version": "1.0.0",
  "description": "A basic template to use for node + express APIs",
  "main": "index.js",
  "scripts": {
    "tsc": "tsc",
    "start": "YOUR_APP_ENV=local ts-node-dev --respawn --transpileOnly ./index.ts",
    "debug": "YOUR_APP_ENV=local ts-node-dev --inspect --respawn --transpileOnly ./index.ts",
    "prod": "tsc && node ./build/serve.js"
  },
  "dependencies": {
    "@types/bluebird": "^3.5.27",
    "@types/express": "^4.17.0",
    "@types/lodash": "^4.14.137",
    "bluebird": "^3.5.5",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "dotenv": "^8.1.0",
    "express": "^4.17.1",
    "express-promise-router": "^3.0.3",
    "lodash": "^4.17.15",
    "ts-node-dev": "^1.0.0-pre.41",
    "typescript": "^3.5.3",
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bradledbetter/node_express_template.git"
  },
